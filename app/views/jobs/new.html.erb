
<h1>Nova Vaga</h1>
<hr>
<%= form_for @job do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <br/>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <br/>
  <%= f.label :company %>
  <%= f.text_field :company %>
  <br/>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br/>

  <%= f.submit "Criar Vaga" %>
<% end %>
